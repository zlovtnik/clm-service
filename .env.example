# CLM Service Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# Oracle Database Connection
# Set these to the values provided by your Autonomous DB wallet / tenancy
ORACLE_USERNAME=
ORACLE_PASSWORD=
ORACLE_TNS_NAME=
ORACLE_WALLET_LOCATION=

# Application Settings
CLM_TENANT_ID=DEFAULT
SERVER_PORT=8080

# Logging (optional)
# LOGGING_LEVEL_ROOT=INFO
# LOGGING_LEVEL_COM_GPRINTEX=DEBUG
